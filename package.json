{
  "dependencies": {
    "LiveScript": "1.3.1",
    "body-parser": "^1.11.0",
    "bower": "^1",
    "co": "^4.4.0",
    "colors": "^1.0.3",
    "cookie-parser": "^1.3.3",
    "express": "^4.11.2",
    "express-session": "^1.10.3",
    "front-end-gulp-pattern": "^1",
    "gulp-browserify": "~0.5",
    "gulp-stylus": "^1.3.4",
    "gulp-uglify": "^1.0.1",
    "jade": "^1.9.1",
    "js-yaml": "^3.2.5",
    "liveify-stable": "^1",
    "mongoose": "^3.8.23",
    "mongoose-encrypted": "0.0.10",
    "multer": "^0.1.7",
    "nib": "^1.0.4",
    "nodemailer": "^1.3.1",
    "passport": "~0.2.1",
    "passport-local": "^1.0.0",
    "prelude-ls": "1.1.1",
    "session": "^0.1.0",
    "yargs": "^3.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unclechu/sibtek-site.git"
  },
  "scripts": {
    "postinstall": "./deploy.sh"
  },
  "licenses": [
    {
      "type": "GNU/AGPLv3",
      "url": "https://raw.githubusercontent.com/unclechu/sibtek-site/master/LICENSE-AGPLv3"
    }
  ],
  "homepage": "https://github.com/unclechu/sibtek-site",
  "bugs": {
    "url": "https://github.com/unclechu/sibtek-site/issues"
  },
  "engines": {
    "iojs": ">= 1.2.0"
  },
  "gulp": {
    "distclean": [
      "./node_modules",
      "./bower_components"
    ],
    "styles": {
      "client": {
        "type": "stylus",
        "path": "./front-end-sources/client/styles/",
        "srcDir": "./front-end-sources/client/styles/",
        "mainSrc": "main.styl",
        "destDir": "./static/",
        "buildFile": "client-build.css",
        "shim": [
          "./stylus-shim/nib"
        ]
      },
      "admin": {
        "type": "stylus",
        "path": "./front-end-sources/admin/styles/",
        "srcDir": "./front-end-sources/admin/styles/",
        "mainSrc": "main.styl",
        "destDir": "./static/",
        "buildFile": "admin-build.css",
        "shim": [
          "./stylus-shim/nib"
        ]
      }
    },
    "scripts": {
      "client": {
        "type": "browserify",
        "path": "./front-end-sources/client/scripts/",
        "srcDir": "./front-end-sources/client/scripts/",
        "mainSrc": "main.ls",
        "destDir": "./static/",
        "buildFile": "client-build.js",
        "transform": [
          "liveify-stable"
        ],
        "extensions": [
          ".ls"
        ],
        "jshintDisabled": true,
        "shim": {
          "jquery": {
            "path": "./bower_components/jquery/dist/jquery.min.js",
            "exports": ""
          },
          "modernizr": {
            "path": "./bower_components/modernizr/modernizr.js",
            "exports": "Modernizr"
          },
          "semantic": {
            "path": "./static/semantic/semantic.min.js",
            "exports": "",
            "depends": {
              "jquery": "jQuery"
            }
          }
        }
      },
      "admin": {
        "type": "browserify",
        "path": "./front-end-sources/admin/scripts/",
        "srcDir": "./front-end-sources/admin/scripts/",
        "mainSrc": "main.ls",
        "destDir": "./static/",
        "buildFile": "admin-build.js",
        "transform": [
          "liveify-stable"
        ],
        "extensions": [
          ".ls"
        ],
        "jshintDisabled": true,
        "shim": {
          "jquery": {
            "path": "./bower_components/jquery/dist/jquery.min.js",
            "exports": ""
          },
          "jquery.ckeditor": {
            "path": "./bower_components/ckeditor/adapters/jquery.js",
            "exports": "",
            "depends": {
              "jquery": "jQuery"
            }
          },
          "semantic": {
            "path": "./static/semantic/semantic.min.js",
            "exports": "",
            "depends": {
              "jquery": "jQuery"
            }
          }
        }
      }
    }
  }
}
